{
  "name": "cucumber-playwright",
  "version": "1.2.0",
  "description": "Starter project to write and debug cucumber-js features with Playwright",
  "main": "index.js",
  "scripts": {
    "api": "DEBUG=pw:api cucumber-js",
    "build": "rimraf build && npm run format && npm run lint && tsc && npm run cucumber-check",
    "cucumber-check": "cucumber-js features/**/*.feature --dry-run --require env/set-environment-variables.ts --require world/custom-world.ts --require step-definitions/**/*.ts --require hooks/**/*.ts  --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format summary --format progress --format progress-bar  --publish-quiet",
    "debug": "PWDEBUG=1 npx playwright cucumber-js src/features/**/*.feature --tags @ZoomSample",
    "video": "PWVIDEO=1 cucumber-js",
    "eslint-fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "eslint-init": "eslint --init",
    "format": "prettier --write \"**/*.{ts,tsx,css,html}\" ",
    "lint": "eslint ./ --ext .js,.ts,.tsx --format visualstudio --no-color --report-unused-disable-directives",
    "report": "start reports/report.html",
    "snippets": "cucumber-js features/**/*.feature --dry-run --format snippets",
    "steps-usage": "cucumber-js features/**/*.feature --dry-run",
    "all": "cucumber-js src/features/**/*.feature --format ./src/support/reporters/reporter.ts",
    "test:parallel": "cucumber-js src/features/**/*.feature --parallel=2",
    "docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:latest /bin/bash",
    "allure": "allure serve reports/allure-results",
    "clean": "allure report --clean",
    "campaign": "cucumber-js src/features/**/*.feature --tags @Campaigncreation",
    "login": "cucumber-js src/features/**/*.feature --tags @ClientLogin",
    "apply": "cucumber-js src/features/**/*.feature --tags @CampaignApply",
    "offer": "cucumber-js src/features/**/*.feature --tags @ScheduleOffer",
    "chat": "cucumber-js src/features/**/*.feature --tags @SocketChat",
    "agora": "cucumber-js src/features/**/*.feature --tags @agorameeting",
    "jobA": "cucumber-js src/features/**/*.feature --tags @jobA --format ./src/support/reporters/Reporter.ts",
    "jobB": "cucumber-js src/features/**/*.feature --tags @jobB --format ./src/support/reporters/Reporter.ts",
    "jobC": "cucumber-js src/features/**/*.feature --tags @job1A",
    "runner": "npx ts-node runner.ts",
    "trace": "npx playwright show-trace ./test-results/trace/trace.zip",
    "API": "cucumber-js src/features/**/*.feature --tags @Sample",
    "close": "cucumber-js src/features/**/*.feature --tags @close",
    "test2": "cucumber-js src/features/**/*.feature --tags @ClientLogin",
    "clean:allure-results": "ts-node allureCleaner.ts",
    "testA": "npm run clean:allure-results && cucumber-js src/features/**/*.feature --tags @jobA --format ./src/support/reporters/reporter.ts",
    "testA1": "npm run clean:allure-results && cucumber-js src/features/**/*.feature --tags @jobA0302187 --format ./src/support/reporters/reporter.ts",
    "testA2": "npm run clean:allure-results && cucumber-js src/features/**/*.feature --tags @jobB02879 --format ./src/support/reporters/reporter.ts",
    "testB": "npm run clean:allure-results && cucumber-js src/features/**/*.feature --tags @jobB --format ./src/support/reporters/reporter.ts",
    "testC": "npm run clean:allure-results && cucumber-js src/features/**/*.feature --tags @jobCTest --format ./src/support/reporters/reporter.ts",
    "record1": "npx playwright codegen https://front.ip-poc.com/join/Test",
    "record": "npx playwright codegen https://us05web.zoom.us/j/86901321564?pwd=dmxsa0JmNktWTldzZGdVRnNkTFZxQT09"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tallyb/cucumber7-playwright.git"
  },
  "author": "Tally Barak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tallyb/cucumber-playwright/issues"
  },
  "homepage": "https://github.com/tallyb/cucumber-playwright#readme",
  "dependencies": {
    "@cucumber/cucumber": "^9.6.0",
    "@cucumber/html-formatter": "20.2.1",
    "@cucumber/messages": "^21.0.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@playwright/test": "^1.49.1",
    "@types/auth0-js": "^9.14.7",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "11.0.1",
    "allure": "^0.0.0",
    "aws-sdk": "^2.1339.0",
    "axios": "^1.3.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "cucmber": "^0.0.1-security",
    "cucumber": "^6.0.7",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "5.5.0",
    "node-gyp": "^9.3.1",
    "node-localstorage": "^2.2.1",
    "pixelmatch": "5.3.0",
    "pngjs": "6.0.0",
    "python": "^0.0.4",
    "winston": "^3.13.0",
    "xvfb": "^0.4.0"
  },
  "devDependencies": {
    "@types/cucumber": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "24.3.0",
    "@types/fs-extra": "11.0.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.11.18",
    "@types/node-localstorage": "^1.3.0",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "allure-commandline": "^2.30.0",
    "allure-cucumberjs": "^2.15.1",
    "allure-playwright": "^2.15.1",
    "dotenv": "^16.4.5",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "experimental-allure-playwright": "^0.0.3",
    "fs-extra": "11.1.0",
    "open": "8.4.0",
    "playwright": "^1.47.1",
    "prettier": "2.8.3",
    "rimraf": "^4.4.1",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}